<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Edit Company Information</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Devall\Company\Block\Account\CompanyData"
                   name="customer.company.edit"
                   template="Devall_Company::account/companyedit.phtml"
                   cacheable="false"
            />
        </referenceContainer>
    </body>
</page>
